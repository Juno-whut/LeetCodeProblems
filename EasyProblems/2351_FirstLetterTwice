class Solution:
    def repeatedCharacter(self, s: str) -> str:
        seen = set()
        for i in s:
            if i in seen:
                return i
            else:
                seen.add(i)
        


def main():
    s = "abccbaacz"
    print(Solution.repeatedCharacter(Solution, s))
    s = 'abb'
    print(Solution.repeatedCharacter(Solution, s))

if __name__ == "__main__":
    main()